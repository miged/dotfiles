alias l="ls -la"       # List in long format, include dotfiles
alias ld="ls -ld */ "   # List in long format, only directories
alias ls="ls --color=auto --time-style=long-iso"

alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."

alias g="git"
alias v="vim"
alias h="history | tail -15"
alias j="jobs"
alias quit="exit"
